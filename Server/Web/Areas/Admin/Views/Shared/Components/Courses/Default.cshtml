@using Web.Areas.Admin.ViewModels
@model Semester
    @foreach (var course in Model.Courses)
    {
        <div class=" btn btn-outline-default col-lg-3 col-md-4 col-sm-6">
            <a asp-controller="Course"
               asp-action="Edit"
               asp-route-id="@course.Id">
                @Html.Identicon(course.CourseName, 50)
                <h3>@course.CourseName</h3>
                <h4>@course.CourseId</h4>
            </a>
        </div>
    }
